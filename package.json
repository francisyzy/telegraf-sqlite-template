{
  "name": "telegraf-postgresql-template",
  "version": "0.1.1",
  "description": "Template for creating future telegram bot apps",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run generate && npm run deploy && tsc -p .",
    "dev": "nodemon src/index.ts",
    "generate": "prisma generate",
    "deploy": "prisma migrate deploy",
    "migrate": "prisma migrate dev",
    "reset": "prisma migrate reset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francisyzy/telegraf-PostgreSQL-template.git"
  },
  "author": "francisyzy",
  "bugs": {
    "url": "https://github.com/francisyzy/telegraf-PostgreSQL-template/issues"
  },
  "homepage": "https://github.com/francisyzy/telegraf-PostgreSQL-template#readme",
  "engines": {
    "node": "15.x"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "@types/dotenv": "^8.2.0",
    "dotenv": "^10.0.0",
    "telegraf": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "prisma": "^3.6.0",
    "ts-node": "^10.4.0",
    "typegram": "^3.5.1",
    "typescript": "^4.5.2"
  }
}
